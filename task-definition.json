{
    "family": "react-app-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "react-container",
            "image": "<ACCOUNT_ID>.dkr.ecr.<REGION>.amazonaws.com/react-app-repo:latest",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}